            <servlet-class>B</servlet-class>
        </servlet>
        <servlet>
            <servlet-name>R</servlet-name>
            <servlet-class>R</servlet-class>
        </servlet>
        <servlet>
            <servlet-name>Schedule</servlet-name>
            <servlet-class>Schedule</servlet-class>
        </servlet>
        <servlet>
            <servlet-name>DeleteProfile</servlet-name>
            <servlet-class>DeleteProfile</servlet-class>
        </servlet>
        <servlet>
            <servlet-name>DeleteSchedule</servlet-name>
            <servlet-class>DeleteSchedule</servlet-class>
        </servlet>
        <servlet>
            <servlet-name>RestoreSchedule</servlet-name>
            <servlet-class>RestoreSchedule</servlet-class>
        </servlet>
       <servlet-mapping>
            <servlet-name>Dummy</servlet-name>
            <url-pattern>/Dummy</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>Home</servlet-name>
            <url-pattern>/Home</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>Test</servlet-name>
            <url-pattern>/Test</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>Profile</servlet-name>
            <url-pattern>/Profile</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>Backup</servlet-name>
            <url-pattern>/Backup</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>P</servlet-name>
            <url-pattern>/P</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>B</servlet-name>
            <url-pattern>/B</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>R</servlet-name>
            <url-pattern>/R</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>Schedule</servlet-name>
            <url-pattern>/Schedule</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>DeleteProfile</servlet-name>
            <url-pattern>/Del</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>DeleteSchedule</servlet-name>
            <url-pattern>/SchDel</url-pattern>
        </servlet-mapping>
        <servlet-mapping>
            <servlet-name>RestoreSchedule</servlet-name>
            <url-pattern>/Res</url-pattern>
